<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_voucher_type_tree" model="ir.ui.view">
        <field name="name">voucher.type.tree</field>
        <field name="model">voucher.type</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="code"/>
                <field name="active"/>
            </tree>
        </field>
    </record>

    <record id="view_voucher_type_form" model="ir.ui.view">
        <field name="name">voucher.type.form</field>
        <field name="model">voucher.type</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name"/>
                    <field name="code"/>
                    <field name="active"/>
                </group>
            </form>
        </field>
    </record>
</odoo>
