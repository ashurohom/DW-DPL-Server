<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        
        <record id="group_manufacturing_team" model="res.groups">
            <field name="name">Manufacturing Team</field>
            <field name="category_id" ref="base.module_category_manufacturing"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/> 
        </record>

        
        <record id="group_inventory_team" model="res.groups">
            <field name="name">Inventory Team</field>
            <field name="category_id" ref="base.module_category_inventory"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/> 
        </record>

        
        <record id="stock.menu_stock_root" model="ir.ui.menu">
            <field name="groups_id" eval="[(6, 0, [ref('dw_stock_requisition.group_inventory_team')])]"/>
        </record>

    </data>
</odoo>